CREATE TABLE employees (
    emp_no      INT             NOT NULL,
    birth_date  DATE            NOT NULL,
    fname  VARCHAR(14)     NOT NULL,
    lname   VARCHAR(16)     NOT NULL,
    gender      ENUM ('M','F')  NOT NULL,    
    hire_date   DATE            NOT NULL,
    dept_name   VARCHAR(40)    ,
    title       VARCHAR(50)     NOT NULL,
    salary      INT             NOT NULL,
    PRIMARY KEY (emp_no)
);




